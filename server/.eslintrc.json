{
   "env": {
      "node": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
   "parser": "@typescript-eslint/parser",
   "plugins": ["@typescript-eslint"],
   "rules": {
      "react/prop-types": "off",
      "no-unused-vars": [
         "warn",
         {
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
         }
      ],
      "semi": ["warn", "always"],
      "semi-style": ["error", "last"]
   }
}
